<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App1.Views.ItemsVacinePage"
             Title="{Binding Title}"
             x:Name="VacinePage">
    <ContentPage.Content>
        <StackLayout>
            <ListView>
                <ListView.ItemsSource>
                    <x:Array Type="{x:Type x:String}" >
                        <x:String>AstraZeneca</x:String>
                        <x:String>Butantan/CoronaVac</x:String>
                        <x:String>Janssen</x:String>
                        <x:String>Moderna</x:String>
                        <x:String>Pfizer–BioNTech</x:String>
                        <x:String>Sinopharm</x:String>
                        <x:String>Sinovac</x:String>
                    </x:Array>
                </ListView.ItemsSource>
            </ListView>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>